class TeleKinesisShield extends KSphereSensor
    collapsecategories;

event Touch(Actor Other)
{
    AIActor(Owner).NotifyOtherTouched(self, Other);
    return;
}

event BeginSenseObject(KarmaActor Actor)
{
    super(KSensor).BeginSenseObject(Actor);
    AIActor(Owner).NotifyOtherTouched(self, Actor);
    return;
}

defaultproperties
{
    SphereSensorRadius=200
    Physics=9
    bTraceShootable=false
    bBlockCamera=false
    bCollideActors=true
    MountOrigin=(X=50,Y=0,Z=0)
}